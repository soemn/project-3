<div class="card-deck">
  <% @photos.each do |f| %>
    <div class='col-lg-4 col-sm-6'>
      <div class="card">

        <a href="<%= photo_path(f) %>"><%= cl_image_tag(f.photo_link, class:"card-img-top") %></a>
        <%= render "./partials/show_card_body", :f => f %>
      </div>
    </div>
  <% end %>
</div>
